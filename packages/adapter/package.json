  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x"
  },
  "dependencies": {
    "@astrojs/webapi": "^2.1.0",
    "@middy/core": "^4.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.16.0",
    "@vitest/coverage-c8": "^0.30.1",
    "astro": "^2.3.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "vitest-mock-extended": "^1.1.3"
  }
